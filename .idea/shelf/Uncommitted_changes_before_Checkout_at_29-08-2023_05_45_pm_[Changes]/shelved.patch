+
+        tagDTO.setTagName(tagDTO.getTagName().toUpperCase());
+        tagDTO.setTagDescription(tagDTO.getTagDescription());
+
+        return appUtils.tagToDto(tagRepository.save(appUtils.dtoToTag(tagDTO)));
